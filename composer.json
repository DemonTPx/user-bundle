{
    "name": "demontpx/user-bundle",
    "description": "Couples the FOSUserBundle to my apps",
    "type": "symfony-bundle",
    "keywords": ["user"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Demontpx\\UserBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\UserBundle\\": "tests/" }
    },
    "require": {
        "php": "^5.5|^7.0",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/form": "^2.8|^3.0",
        "doctrine/orm": "^2.4",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "demontpx/util-bundle": "^0.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "twig/twig": "^1.5",
        "doctrine/doctrine-bundle": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.0"
    },
    "suggest": {
        "doctrine/doctrine-fixtures-bundle": "For functional testing with configured users",
        "pinano/select2-bundle": "Improves forms"
    },
    "config": {
        "bin-dir": "bin"
    }
}
