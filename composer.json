{
    "name": "demontpx/user-bundle",
    "description": "Provider users and user management to my Symfony apps",
    "type": "symfony-bundle",
    "keywords": ["user"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Demontpx\\UserBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\UserBundle\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "symfony/config": "^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-foundation": "^4.0|^5.0",
        "symfony/security": "^4.0|^5.0",
        "symfony/event-dispatcher-contracts": "^1.1|^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/form": "^4.0|^5.0",
        "doctrine/orm": "^2.6",
        "demontpx/util-bundle": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/yaml": "^4.0|^5.0",
        "twig/twig": "^2.4|^3.0",
        "doctrine/doctrine-bundle": "^1.8|^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "suggest": {
        "doctrine/doctrine-fixtures-bundle": "For functional testing with configured users"
    },
    "config": {
        "bin-dir": "bin"
    }
}
