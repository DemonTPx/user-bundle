{
    "name": "demontpx/user-bundle",
    "description": "Provider users and user management to my Symfony apps",
    "type": "symfony-bundle",
    "keywords": ["user"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Demontpx\\UserBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Demontpx\\UserBundle\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/http-foundation": "^3.2",
        "symfony/security": "^3.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/form": "^3.2",
        "doctrine/orm": "^2.5",
        "demontpx/util-bundle": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/yaml": "^3.2",
        "twig/twig": "^2.4",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3"
    },
    "suggest": {
        "doctrine/doctrine-fixtures-bundle": "For functional testing with configured users"
    },
    "config": {
        "bin-dir": "bin"
    }
}
