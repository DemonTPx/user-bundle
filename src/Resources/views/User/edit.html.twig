{% extends '::base.html.twig' %}

{% block content %}
    <div class="container">
        {{ form_start(form) }}
        {{ form_rest(form) }}
        <div class="col-lg-offset-3 col-lg-9">
            <button type="submit" class="btn btn-primary">Save</button>
            <button type="button" class="btn btn-link" onclick="history.back()">Close</button>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
