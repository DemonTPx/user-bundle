{% extends '::base.html.twig' %}

{% trans_default_domain 'FOSUserBundle' %}

{% block content %}
    {{ form_start(form) }}
    {{ form_rest(form) }}

    <button type="submit" class="btn btn-primary">{{ 'demontpx_user.manage.save'|trans }}</button>
    <button type="button" class="btn btn-link" onclick="history.back()">{{ 'demontpx_user.manage.cancel'|trans }}</button>
    {{ form_end(form) }}

    {% javascripts '@DemontpxUserBundle/Resources/public/js/user_edit.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
